import java.util.Scanner;
public class ReverseArray {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int[] array = new int[n];
        int res=0;
        for(int i=0;i<n;i++)
        {
            array[i] = scanner.nextInt();
            if(array[i]<0)
            {
                res+=1;
            }
        }
        System.out.println(res);
    }
}
